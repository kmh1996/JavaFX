<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane 
fx:controller="c4_thread.RebootController"
prefHeight="500.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
   <center>
      <StackPane BorderPane.alignment="CENTER">
         <children>
            <VBox prefHeight="400.0" prefWidth="500.0">
               <children>
                  <Canvas fx:id="bgCanvas" height="400.0" width="600.0" />
               </children>
            </VBox>
            <VBox fx:id="carBox" prefHeight="400.0" prefWidth="500.0">
               <children>
                  <Canvas fx:id="canvas1" height="100.0" width="600.0" />
                  <Canvas fx:id="canvas2" height="100.0" width="600.0" />
                  <Canvas fx:id="canvas3" height="100.0" width="600.0" />
                  <Canvas fx:id="canvas4" height="100.0" width="600.0" />
               </children>
            </VBox>
         </children>
      </StackPane>
   </center>
   <bottom>
      <HBox alignment="CENTER" BorderPane.alignment="CENTER">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <Button fx:id="btnStart" mnemonicParsing="false" text="Start" />
         </children>
      </HBox>
   </bottom>
</BorderPane>
