<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.StackPane?>


<StackPane 
fx:controller="application.RootController"  
prefHeight="500.0" prefWidth="300.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
   <children>
      <BorderPane fx:id="mainWrap" id="mainWrap" prefHeight="200.0" prefWidth="200.0">
         <center>
            <VBox prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: blue;" BorderPane.alignment="CENTER" />
         </center>
         <top>
            <BorderPane style="-fx-background-color:#eaeaea;" BorderPane.alignment="CENTER">
               <center>
                  <Label text="메인화면" BorderPane.alignment="CENTER" />
               </center>
               <right>
                  <Button fx:id="btnNext" mnemonicParsing="false" text="Next" BorderPane.alignment="CENTER" />
               </right>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </BorderPane>
         </top>
      </BorderPane>
      
   </children>
</StackPane>
